{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.1|^8.0",
        "ext-fileinfo": "*",
        "ext-zip": "*",
        "adrianorosa/laravel-geolocation": "^1.0",
        "ajcastro/eager-load-pivot-relations": "^0.2.2",
        "alexusmai/laravel-file-manager": "^2.5",
        "awobaz/compoships": "^2.1",
        "elasticquent/elasticquent": "dev-master",
        "fideloper/proxy": "^4.2",
        "fruitcake/laravel-cors": "^2.0",
        "google/apiclient": "2.12.1",
        "guzzlehttp/guzzle": "^7.2",
        "intervention/image": "^2.6",
        "irazasyed/telegram-bot-sdk": "^3.5",
        "laravel/framework": "^8.6.0",
        "laravel/scout": "^9.4",
        "laravel/tinker": "^2.0",
        "mavinoo/laravel-batch": "^2.3",
        "nelexa/google-play-scraper": "0.4.0.6",
        "pbmedia/laravel-ffmpeg": "*",
        "phpgangsta/googleauthenticator": "dev-master",
        "pragmarx/google2fa": "^8.0",
        "pusher/pusher-php-server": "^7.0",
        "staudenmeir/eloquent-eager-limit": "*",
        "staudenmeir/eloquent-json-relations": "*",
        "sunra/php-simple-html-dom-parser": "^1.5",
        "voku/portable-utf8": "^5.4",
        "voku/simple_html_dom": "^4.7",
        "yajra/laravel-datatables-oracle": "^9.11"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.6",
        "facade/ignition": "^2.3.6",
        "fzaninotto/faker": "^1.9.1",
        "mockery/mockery": "^1.3.1",
        "nunomaduro/collision": "^5.0",
        "phpunit/phpunit": "^9.3"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
